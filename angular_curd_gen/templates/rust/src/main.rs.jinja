use tokio::runtime::Builder;
use {{ app_name }}::{reload_app};

#[tokio::main]
async fn main() {
    let runtime = Builder::new_multi_thread()
        .worker_threads(10)
        .enable_all()
        .build()
        .unwrap();

    runtime.spawn(async {
        reload_app().await;
    }).await.unwrap();
}
